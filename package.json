{
  "name": "vue-uswds",
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "commit": "cz",
    "commit:retry": "cz --retry",
    "release": "standard-version",
    "new": "hygen new",
    "lint": "eslint --ext .js,.vue --fix .",
    "cypress:open": "cypress open",
    "cypress:open-ct": "cypress open-ct",
    "test:component": "cypress run-ct",
    "test:e2e": "cypress run",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "postinstall": "ln -s node_modules/uswds/dist assets"
  },
  "dependencies": {
    "@vueuse/components": "^7.5.4",
    "@vueuse/core": "^7.5.3",
    "@vueuse/integrations": "^7.5.3",
    "just-kebab-case": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@cypress/vite-dev-server": "^2.2.0",
    "@cypress/vue": "^3.0.3",
    "@storybook/addon-actions": "6.3.13",
    "@storybook/addon-essentials": "6.3.13",
    "@storybook/addon-links": "6.3.13",
    "@storybook/vue3": "6.3.13",
    "@vitejs/plugin-vue": "^2.1.0",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "cypress": "^9.3.1",
    "cypress-real-events": "^1.5.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.2",
    "storybook-builder-vite": "^0.1.3",
    "uswds": "^2.12.2",
    "vite": "^2.6.4",
    "vue": "^3.2.21",
    "vue-loader": "^16.8.2",
    "vue-router": "^4.0.12"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ]
  }
}

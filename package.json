{
  "name": "vue-uswds",
  "description": "Vue 3 implementation of the USWDS (U.S. Web Design System) component library",
  "version": "0.7.4",
  "author": "Patrick Cate",
  "license": "MIT",
  "keywords": [
    "vue",
    "uswds",
    "U.S. Web Design System"
  ],
  "homepage": "https://patrickcate.github.io/vue-uswds",
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickcate/vue-uswds"
  },
  "bugs": {
    "url": "https://github.com/patrickcate/vue-uswds/issues"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "main": "./dist/vue-uswds.umd.js",
  "module": "./dist/vue-uswds.es.js",
  "unpkg": "./dist/vue-uswds.umd.js",
  "jsdelivr": "./dist/vue-uswds.umd.js",
  "exports": {
    ".": {
      "require": "./dist/vue-uswds.umd.js",
      "import": "./dist/vue-uswds.es.js"
    },
    "./core": {
      "require": "./dist/vue-uswds.core.umd.js",
      "import": "./dist/vue-uswds.core.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:all && npm run build:core",
    "build:all": "vite build -c ./build/vite.build.all.js",
    "build:core": "vite build -c ./build/vite.build.core.js",
    "serve": "vite preview",
    "commit": "cz",
    "release": "standard-version",
    "new": "hygen new",
    "lint": "eslint --ext .js,.vue --fix .",
    "cypress:open": "cypress open",
    "cypress:open-ct": "cypress open-ct",
    "pretest:component": "rm -rf .nyc_output coverage || true",
    "test:component": "cypress run-ct",
    "test:e2e": "cypress run",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "symlink-assets": "ln -s node_modules/uswds/dist assets"
  },
  "dependencies": {
    "@vueuse/components": "^8.0.0",
    "@vueuse/core": "^8.0.0",
    "@vueuse/integrations": "^8.0.0",
    "fast-sort": "^3.1.3",
    "focus-trap": "^6.7.3",
    "just-kebab-case": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/vite-dev-server": "^2.2.0",
    "@cypress/vue": "^3.0.3",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-docs": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/builder-vite": "^0.1.28",
    "@storybook/vue3": "^6.4.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "babel-loader": "^8.2.3",
    "commitizen": "^4.2.4",
    "cypress": "^9.3.1",
    "cypress-real-events": "^1.5.1",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "hygen": "^6.1.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.2",
    "uswds": "^2.12.2",
    "vite": "^2.6.4",
    "vite-plugin-istanbul": "^2.5.1",
    "vue": "^3.2.33",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.0.12"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ]
  }
}
